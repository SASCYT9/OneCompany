{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 3,
    "name": "onecompany/gallery-block",
    "version": "1.0.0",
    "title": "OneCompany Liquid Gallery",
    "category": "onecompany",
    "icon": "format-gallery",
    "description": "Преміальна галерея зображень з ефектом Liquid Glass",
    "attributes": {
        "images": {
            "type": "array",
            "default": []
        },
        "columns": {
            "type": "number",
            "default": 3
        },
        "gap": {
            "type": "number",
            "default": 20
        }
    },
    "supports": {
        "html": false,
        "align": ["wide", "full"]
    },
    "textdomain": "onecompany-theme",
    "editorScript": "file:../../build/gallery-block/index.js"
}
